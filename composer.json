{
    "name": "avatarfy/avatarfy",
    "description": "Avatarfy - A comprehensive Laravel package for generating culturally-aware SVG avatars with expressions, age groups, and customizable features",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "avatar", "svg", "generator", "profile", "cultural", "expressions", "demographics", "avatarfy"],
    "authors": [
        {
            "name": "Nirav Goriya",
            "email": "goriyanirav224@gmail.com"
        }
    ],
    "homepage": "https://github.com/niravpatel143/avatarfy",
    "support": {
        "issues": "https://github.com/niravpatel143/avatarfy/issues",
        "source": "https://github.com/niravpatel143/avatarfy"
    },
    "autoload": {
        "psr-4": {
            "Avatarfy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Avatarfy\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Avatarfy\\AvatarServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
